<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="StockWise.Client.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:paginas="clr-namespace:StockWise.Client.Paginas"
    Shell.NavBarIsVisible="False">

    <ShellContent
        Route="login"
        ContentTemplate="{DataTemplate paginas:LoginPage}" />

    <ShellContent
        Route="productos"
        ContentTemplate="{DataTemplate paginas:ProductosPage}" />

    <ShellContent
        Route="CambiarPassword"
        ContentTemplate="{DataTemplate paginas:CambiarPassword}" />

    <ShellContent Route="ModificarStockPage" ContentTemplate="{DataTemplate paginas:ModificarStockPage}" />


</Shell>
